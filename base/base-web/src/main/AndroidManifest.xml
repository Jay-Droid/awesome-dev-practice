<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jay.base_web">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <uses-permission android:name="android.permission.INTERNET" />

    <uses-permission android:name="android.permission.READ_PHONE_STATE" />

    <application>
        <!--appLife ,name需要是全类名,value为固定值用于筛选 -->
        <meta-data
            android:name="com.jay.base_web.app.BaseWebApp"
            android:value="IModuleConfig" />

        <activity
            android:name="com.jay.base_web.web.WebActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name="com.jay.base_web.web.TestActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name="com.jay.base_web.fileview.PdfOnlineViewActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name="com.jay.base_web.fileview.TestActivity"
            android:screenOrientation="portrait" />
    </application>
</manifest>
